.icon image{
  width: 80px;
  border-radius: 8px;
}
.icon{
  text-align: center;
  margin-top: 10%;
  margin-bottom: 10%;
}
.describ{
  color: gray;
}


.list{
  display: flex;
  background-color: white;
  padding: 0px 10px;
  height: 50px;
  border-bottom: 1px solid gainsboro;
}
.list>view{
  display: flex;
  align-items: center;
}
.list .list_icon{
  width: 40px;
}
.list .list_icon image{
  width:20px;
}
.list .list_text{
  flex:1;
}
.list .list_res{
  width: 120px;
  color: gray;
  font-size: 0.9rem;
  display: flex;
  justify-content: center;
}
.cc{
  margin-left: 3px;
  background-color: #5cb85c;
 
  display: inline;
  padding: .2em .6em .3em;
  font-weight: 700;
  line-height: 1;
  color: #fff;
  text-align: center;
  white-space: nowrap;
  vertical-align: baseline;
  border-radius: .25em;
}
